./elastic-mapreduce -c /Users/perk387/Development/Velocity/credentials.json   --create --alive --name "Velocity" --enable-debugging \
--num-instances 4 --slave-instance-type m3.xlarge --master-instance-type m3.xlarge --ami-version "2.4.2" \
--bootstrap-action s3://elasticmapreduce/bootstrap-actions/configure-hadoop \
--args "-m,mapred.reduce.tasks.speculative.execution=false" \
--args "-m,mapred.map.tasks.speculative.execution=false" \
--args "-m,mapred.map.child.java.opts=-Xmx1024m" \
--args "-m,mapred.reduce.child.java.opts=-Xmx1024m" \
--args "-m,mapred.job.reuse.jvm.num.tasks=1" \
--bootstrap-action s3n://velocity1/emrsetuprhipe --log-uri s3n://velocity1




./elastic-mapreduce -c /Users/perk387/Development/Velocity/credentials.json   --create --alive --ami-version "2.4.2" --ssh


./elastic-mapreduce --create --alive --name "VelocityCluster" --enable-debugging \
--num-instances 5 --slave-instance-type m3.xlarge --master-instance-type m3.xlarge --ami-version "2.4.2" \
--with-termination-protection \
--key-pair Velocity \
--log-uri s3n://velocity7/logs \
--bootstrap-action "s3://velocity1/install-preconfigure" \
--bootstrap-action "s3://velocity1/install-r" \
--bootstrap-action s3://elasticmapreduce/bootstrap-actions/run-if \
--args "instance.isMaster=true,s3://velocity1/install-rstudio" \
--bootstrap-action s3://elasticmapreduce/bootstrap-actions/run-if \
--args "instance.isMaster=true,s3://velocity1/install-shiny-server" \
--bootstrap-action "s3://velocity1/install-protobuf" \
--bootstrap-action "s3://velocity1/install-rhipe" \
--bootstrap-action "s3://velocity1/install-additional-pkgs"


./elastic-mapreduce --create --alive --name "VelocityCluster" --enable-debugging \
--num-instances 5 --slave-instance-type m1.medium --master-instance-type m3.xlarge --ami-version "2.4.2" \
--with-termination-protection \
--key-pair Velocity \
--log-uri s3n://velocity7/logs \
--bootstrap-action "s3://velocity1/install-preconfigure" \
--bootstrap-action "s3://velocity1/install-r" \
--bootstrap-action "s3://velocity1/install-rstudio" \
--bootstrap-action "s3://velocity1/install-shiny-server" \
--bootstrap-action "s3://velocity1/install-protobuf" \
--bootstrap-action "s3://velocity1/install-rhipe" \
--bootstrap-action "s3://velocity1/install-additional-pkgs"

./elastic-mapreduce --create --alive --name "VelocityClusterMini-m1.large" --enable-debugging \
--num-instances 2 --slave-instance-type m1.large --master-instance-type m1.large --ami-version "2.4.2" \
--with-termination-protection \
--key-pair Velocity \
--log-uri s3n://velocity7/logs \
--bootstrap-action "s3://velocity1/install-preconfigure" \
--bootstrap-action "s3://velocity1/install-r" \
--bootstrap-action "s3://velocity1/install-rstudio" \
--bootstrap-action "s3://velocity1/install-shiny-server" \
--bootstrap-action "s3://velocity1/install-protobuf" \
--bootstrap-action "s3://velocity1/install-rhipe" \
--bootstrap-action "s3://velocity1/install-additional-pkgs"
